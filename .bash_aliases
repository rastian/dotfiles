# Misc
alias open="xdg-open"
alias cdschool="cd ~/Dropbox/School/Fall-15"
alias db="dropbox"
alias eclipse='~/.eclipse/eclipse'
alias synaptic="sudo synaptic"

# Movement
alias .='cd ..'
alias ..='cd ../..'
alias ...='cd ../../..'
alias ....='cd ../../../..'
alias c='clear'

# Games
alias mcserver="cd ~/.minecraft_server && java -jar minecraft_server.1.8.1.jar world nogui"
alias minecraft="cd ~ && java -jar .Minecraft.jar"
alias dwarf="~/.Dwarf-Fortress/startlnp"

# Emacs
alias emacscn="emacsclient -c -n"
alias kill-emacs="emacsclient -e '(kill-emacs)'"
alias restart-emacs="emacsclient -e '(kill-emacs)' && emacs --daemon"
