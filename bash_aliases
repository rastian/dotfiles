# Misc
alias open="xdg-open"
alias cdschool="cd ~/Dropbox/School/Spring-15"
alias db="dropbox"
alias android-studio='~/software/android-studio/bin/studio.sh'
alias eclipse='~/.eclipse/eclipse'
alias fuck='sudo $(history -p \!\!)'
# Movement
alias .='cd ..'
alias ..='cd ../..'
alias ...='cd ../../..'
alias ....='cd ../../../..'
alias wiki='python3 ~/git/wikipedia-cli/wiki.py'
alias c='clear'

# Apt-Get
alias aptinstall='sudo apt-get install'
alias aptupdate='sudo apt-get update'

# Games
alias pokemmo="cd ~/.PokeMMO && sh PokeMMO.sh"
alias mcserver="cd ~/.minecraft_server && java -jar minecraft_server.1.8.1.jar world nogui"
alias minecraft="cd ~ && java -jar .Minecraft.jar"
alias dwarf="~/.Dwarf-Fortress/startlnp"
# Emacs
alias emacscn="emacsclient -c -n"
alias kill-emacs="emacsclient -e '(kill-emacs)'"
alias restart-emacs="emacsclient -e '(kill-emacs)' && emacs --daemon"

# Synaptic
alias synaptic="sudo synaptic"
